<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="473.0" prefWidth="645.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jkesle.crm.controller.AddCustomerController">
    <children>
        <Label alignment="CENTER" layoutX="203.0" layoutY="51.0" prefHeight="34.0" prefWidth="240.0" text="New Customer Information" textFill="#464646">
            <font>
                <Font name="System Bold" size="18.0" />
            </font></Label>
        <TextField fx:id="nameField" layoutX="248.0" layoutY="108.0" />
        <TextField fx:id="addressField" layoutX="248.0" layoutY="160.0" />
        <TextField fx:id="postalField" layoutX="249.0" layoutY="212.0" />
        <TextField fx:id="phoneField" layoutX="249.0" layoutY="261.0" />
        <ComboBox fx:id="countryBox" layoutX="249.0" layoutY="311.0" prefWidth="150.0" promptText="Select Country" />
        <ComboBox fx:id="divisionBox" layoutX="249.0" layoutY="363.0" prefWidth="150.0" promptText="Select State/Province" />
        <Label layoutX="189.0" layoutY="112.0" text="Name">
            <font>
                <Font name="System Bold Italic" size="12.0" />
            </font>
        </Label>
        <Label layoutX="184.0" layoutY="164.0" text="Address">
            <font>
                <Font name="System Bold Italic" size="12.0" />
            </font>
        </Label>
        <Label layoutX="192.0" layoutY="216.0" text="Postal">
            <font>
                <Font name="System Bold Italic" size="12.0" />
            </font>
        </Label>
        <Label layoutX="194.0" layoutY="265.0" text="Phone">
            <font>
                <Font name="System Bold Italic" size="12.0" />
            </font>
        </Label>
        <Button fx:id="submitBtn" layoutX="188.0" layoutY="425.0" mnemonicParsing="false" text="Add New Customer">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Button>
        <Button fx:id="cancelBtn" layoutX="347.0" layoutY="425.0" mnemonicParsing="false" onAction="#returnToMain" prefHeight="25.0" prefWidth="105.0" text="Cancel">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Button>
        <Label fx:id="errorLabel" alignment="CENTER" layoutX="95.0" layoutY="399.0" prefHeight="17.0" prefWidth="454.0" />
    </children>
</AnchorPane>
